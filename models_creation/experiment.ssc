var class_map = {'ORI': 0, 'GEM': 1, 'CNC': 2, 'CMI': 3, 'CMA': 4, 'MON': 5, 'LEP': 6, 'SEX': 7, 'PYX': 8, 'TRI': 9, 'ARI': 10, 'LEO': 11, 'LMI': 12, 'LYN': 13, 'VIR': 14, 'VEL': 15, 'CEN': 16, 'CRT': 17, 'ANT': 18, 'HYA': 19, 'PUP': 20, 'COL': 21, 'CAR': 22, 'CAS': 23, 'PIC': 24, 'DOR': 25, 'AND': 26, 'TAU': 27, 'AUR': 28, 'HOR': 29, 'CAE': 30, 'SCL': 31, 'CET': 32, 'FOR': 33, 'PHE': 34, 'CAM': 35, 'ERI': 36, 'PEG': 37, 'PER': 38, 'PSC': 39, 'UMA': 40, 'UMI': 41, 'CEP': 42, 'CHA': 43, 'CIR': 44, 'COM': 45, 'CRA': 46, 'CRB': 47, 'CRU': 48, 'CRV': 49, 'CVN': 50, 'CYG': 51, 'DEL': 52, 'DRA': 53, 'EQU': 54, 'GRU': 55, 'HER': 56, 'HYI': 57, 'IND': 58, 'LAC': 59, 'LIB': 60, 'LUP': 61, 'LYR': 62, 'MEN': 63, 'MIC': 64, 'MUS': 65, 'NOR': 66, 'OCT': 67, 'OPH': 68, 'PAV': 69, 'PSA': 70, 'RET': 71, 'SCO': 72, 'SCT': 73, 'SER': 74, 'SGE': 75, 'SGR': 76, 'APS': 77, 'AQL': 78, 'AQR': 79, 'ARA': 80, 'VOL': 81, 'VUL': 82, 'BOO': 83, 'CAP': 84, 'TUC': 85, 'TRA': 86, 'TEL': 87}
///Constant setup
const MinFOV = 30;
const MaxFOV = 90;
const AspectRatio = 16/9;
const JDay = 2451545.220345;
///const MagLimit = 6;
const ShutterSpeed = 0.2;
const StartTime = 2;
const ImageCount = 2000;
const DEGREE = Math.PI/180;
var constellation_names = Object.keys(constellation_stars);
var magLimit = 4;
///Environmental setup
core.setTimeRate(0);
core.setJDay(JDay);
LandscapeMgr.setFlagLandscape(false);
LandscapeMgr.setFlagAtmosphere(true);
StelSkyDrawer.setFlagTwinkle(false);
NebulaMgr.setFlagShow(false);
SporadicMeteorMgr.setFlagShow(false);
MeteorShowers.setEnableLabels(false);
CustomObjectMgr.removeCustomObjects();
SolarSystem.setFlagPlanets(false);
StarMgr.setFlagLabels(false);
MilkyWay.setFlagShow(true);
ZodiacalLight.setFlagShow(false);
StelSkyDrawer.setFlagStarMagnitudeLimit(true);
StelSkyDrawer.setCustomStarMagnitudeLimit(magLimit);


///Debugging
ConstellationMgr.setFlagLines(true);
ConstellationMgr.setFlagLabels(true);


///Viewport setup
StelMovementMgr.setEquatorialMount(false);
core.setProjectionMode("ProjectionPerspective");
core.setGuiVisible(false);


///Variables
var RA, DEC, THETA, FOV, X, Y;

///Helpers
function transform(RA, DEC, THETA) {
    RA *= DEGREE;
    DEC *= DEGREE;
    THETA *= DEGREE;
    return function(star) {
        var ra = star[0] * DEGREE;
        var dec = star[1] * DEGREE;
        var cos_c = Math.sin(DEC) * Math.sin(dec) + Math.cos(DEC) * Math.cos(dec) * Math.cos(ra - RA);
        var c = Math.acos(cos_c);
        var cos_A = (Math.sin(dec) - Math.sin(DEC) * cos_c) / (Math.cos(DEC) * Math.sin(c));
        var A = Math.acos(cos_A);

        if (Math.sin(ra - RA) < 0) {
            A = 2 * Math.PI - A;
        }

        return [Math.tan(c), THETA - A];
    };
}
function isVisible(TR, star) {
	var R = TR(star);
    if(R[0]<0) {
        return 0;
    } else if(Math.abs(R[0]*Math.sin(R[1]))>=X) {
        return 0;
    } else if(Math.abs(R[0]*Math.cos(R[1]))>=Y) {
        return 0;
    } else {
		return 1;
	}
}
function isMagVisible(star) {
    var mag = star[2];
    if(mag > magLimit) {
        return 0;
    }
    return 1;
}
function getFraction(TR, stars) {
	return stars.map(function(star) {return isVisible(TR, star);}).reduce(function(x,y) {return x+y;})/stars.length;
}
function getMagFraction(stars) {
	return stars.map(function(star) {return isMagVisible(star);}).reduce(function(x,y) {return x+y;});
}

function setView(RA, DEC, THETA, FOV) {
    core.setObserverLocation(RA, DEC, 0, 0, "", "");
    core.moveToAltAzi(89.99, THETA, 0);
	StelMovementMgr.zoomTo(FOV, 0);
	Y = Math.tan(FOV*DEGREE/2);
	X = AspectRatio*Y;
}

function getBoundingBox(TR, stars) {
    var visibleStars = stars.filter(function(star) { return isVisible(TR, star) && isMagVisible(star); });
    var minX = Infinity, maxX = -Infinity, minY = Infinity, maxY = -Infinity;
    
    visibleStars.forEach(function(star) {
        var R = TR(star);
        var x = 0.5 + R[0] * Math.sin(R[1]) / (2 * X);
        var y = 0.5 - R[0] * Math.cos(R[1]) / (2 * Y); // Change the plus sign to a minus sign here
        
        minX = Math.min(minX, x);
        maxX = Math.max(maxX, x);
        minY = Math.min(minY, y);
        maxY = Math.max(maxY, y);
    });

    var centerX = (minX + maxX) / 2;
    var centerY = (minY + maxY) / 2;
    var width = maxX - minX;
    var height = maxY - minY;

    return [1-centerX, 1-centerY, width, height];
}

function getLabel(RA, DEC, THETA) {
    var TR = transform(RA, DEC, THETA);

    return constellation_names
        .filter(function(name) {
            var fractionVisible = getFraction(TR, constellation_stars[name]);
            var fractionMagVisible = getMagFraction(constellation_stars[name]);
            return (constellation_stars[name].length >= 4 && fractionVisible == 1 && fractionMagVisible >= 4);
        })
        .map(function(name) {
            var bbox = getBoundingBox(TR, constellation_stars[name]);
            return class_map[name] + " " + bbox.join(" ");
        });
}

///Data
core.wait(StartTime);
core.resetOutput();
for(i=0; i<10; i++) {
    ConstellationMgr.setFlagLines(true);
    ConstellationMgr.setFlagLabels(true);
    RA = 360*Math.random()-180;
    DEC = Math.asin(2*Math.random()-1)/DEGREE;
    THETA = 360*Math.random();
	FOV = MinFOV+(MaxFOV-MinFOV)*Math.random();
    setView(RA, DEC, THETA, FOV);
    core.output(i + "\n" + getLabel(RA, DEC, THETA).join("\n"));
    core.wait(ShutterSpeed);
    core.screenshot(i + "lines", false, "", true);
    ConstellationMgr.setFlagLines(false);
	ConstellationMgr.setFlagLabels(false);
    core.wait(1);
    core.wait(ShutterSpeed);
    core.screenshot(i+"no_lines", false, "", true);
}


///Wakarimashita. Nothing last forever. Not even this script.
core.setGuiVisible(true);
core.exit();